<div class="filtrer">
    <form id="formPost" onsubmit="return false;">
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="Url">Url :</label>
                    <div class="control-label ">
                        <input id="Url" name="Url" type="text" class="form-control input-sm" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="Description">Description :</label>
                    <div class="control-label ">
                        <input id="Description" name="Description" type="text" class="form-control input-sm" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="Prix">Prix :</label>
                    <div class="control-label ">
                        <input id="Prix" name="Prix" type="text" class="form-control input-sm" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="Date">Date :</label>
                    <div class="control-label ">
                        <input id="Date" name="Date" type="text" class="form-control input-sm" disabled />
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>